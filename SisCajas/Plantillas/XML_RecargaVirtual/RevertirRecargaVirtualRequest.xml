                  <!--0 to 5 repetitions:-->
                  <v12:validity value="?" sequence="?" validityFromLifecycle="?"/>
               </v12:validity-Info>
            </v12:payerValidity>
            <!--Optional:-->
            <v12:walletValidity>
               <v12:numberOfvalidities value="?"/>
